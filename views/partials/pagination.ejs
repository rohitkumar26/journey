<% if("previous" in result) {%>
<a
  href="/journey?page=<%=result.previous.page%>&limit=<%=result.previous.limit%>"
  class="button is-small"
  >Previous</a
>

<% } else{%>
<a href="#" class="button is-small" disabled>Previous</a>
<% } %>
<span class="button is-small has-text-white has-background-dark"
  ><%=currentpage%></span
><% if("next" in result) {%>
<a
  href="/journey?page=<%=result.next.page%>&limit=<%=result.next.limit%>"
  class="button is-small"
  >Next page</a
>
<% } else{%>
<a href="#" class="button is-small" disabled>Next page</a>
<% } %>
